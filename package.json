{
  "name": "ez-downloader",
  "version": "1.0.0",
  "description": "Utility to download tracks from Soundcloud and Youtube",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint lib/*.js",
    "build": "babel lib -d dist/",
    "start": "npm run lint && node dist/index.js",
    "start:nodemon": "nodemon dist/index.js",
    "start:dev": "npm run lint && babel-node lib/index.js",
    "start:dev:nodemon": "nodemon lib/index.js --exec \"npm run lint && babel-node lib/index.js\"",
    "start:prod": "node dist/index.js",
    "re": "npm run lint && npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasriondel/ez-downloader.git"
  },
  "keywords": [
    "soundcloud",
    "youtube",
    "download",
    "downloader",
    "ez"
  ],
  "author": "Lucas Riondel & Thomas Andr√©",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucasriondel/ez-downloader/issues"
  },
  "homepage": "https://github.com/lucasriondel/ez-downloader#readme",
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "browser-id3-writer": "^3.0.3",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "debug": "^2.6.3",
    "fluent-ffmpeg": "^2.1.0",
    "inquirer": "^3.0.6",
    "md5": "^2.2.1",
    "progress": "^2.0.0",
    "request": "^2.81.0",
    "request-progress": "^3.0.0",
    "request-promise-native": "^1.0.3",
    "url-parse": "^1.1.8",
    "ytdl-core": "^0.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0"
  }
}
